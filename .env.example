# Example .env file for MLShopHelper
# Copy this file to `.env` and replace the values with your own Appwrite project details

VITE_APPWRITE_ENDPOINT=https://your-appwrite-endpoint/v1
VITE_APPWRITE_PROJECT=your_project_id
VITE_APPWRITE_DATABASE=your_database_id
VITE_APPWRITE_ITEMS_COLLECTION=your_items_collection_id
VITE_APPWRITE_ANON_LINKS_COLLECTION=your_anon_links_collection_id
VITE_APPWRITE_PRICE_HISTORY_COLLECTION=your_price_history_collection_id
VITE_APPWRITE_STATS_COLLECTION=your_stats_collection_id
VITE_APPWRITE_VERSION_COLLECTION=your_version_collection_id
VITE_APP_VERSION=0.0.0

# URL for the item data source used by the populate-items function
ITEMS_DATA_SOURCE_URL=
